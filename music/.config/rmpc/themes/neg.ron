#![enable(implicit_some)]
#![enable(unwrap_newtypes)]
#![enable(unwrap_variant_newtypes)]
(
    default_album_art_path: None,
    draw_borders: false,
    show_song_table_header: false,
    symbols: (song: "üéµ", dir: "üìÅ", marker: "\u{e0b0}"),
    layout: Split(direction: Vertical, panes: [( pane: Pane(TabContent), size: "100%",), ( pane: Pane(ProgressBar), size: "1",),],),
    progress_bar: ( symbols: ["‚îÄ", "‚ïº", ""], track_style: (fg: "#162b44"), elapsed_style: (fg: "#162b44"), thumb_style: (fg: "#162b44"),),
    scrollbar: ( symbols: ["", "", "", ""], track_style: (), ends_style: (), thumb_style: (fg: "#000000"),),
    browser_column_widths: [20, 38, 42],
    text_color: "#95a7bc",
    background_color: None,
    header_background_color: None,
    modal_background_color: None,
    modal_backdrop: false,
    tab_bar: (active_style: (fg: "black", bg: "#0a3749", modifiers: "Bold"), inactive_style: ()),
    borders_style: (fg: "#005faf"),
    highlighted_item_style: (modifiers: "Italic"),
    highlight_border_style: (fg: "#005faf"),
    current_item_style: (fg: "#005faf", bg: "0", modifiers: "Bold"),
    song_table_format: [
        ( prop: (kind: Property(Track), style: (fg: "green"), default: (kind: Text("Unknown"))), width: "2%",),
        ( prop: (kind: Group([
          (kind: Property(Artist)),
          (kind: Text(" ‚ùØ ")),
          (kind: Property(Title)),
        ]), default: (kind: Property(Filename))), width: "20%"),
        // ( prop: (kind: Property(Artist), style: (fg: "31"), default: (kind: Text("Unknown"))), width: "10%",),
        // ( prop: (kind: Property(Title), default: (kind: Text("Unknown"))), width: "100%",),
        ( prop: (kind: Property(Duration), default: (kind: Text("-")), style: (fg: "31")), width: "100%", alignment: Right,),
        ( prop: (kind: Property(Album), default: (kind: Text("Unknown"))), width: "20%", alignment: Right,),
    ],
    browser_song_format: [
        ( kind: Group([ (kind: Property(Track)), (kind: Text(" ")), ])),
        ( kind: Group([ (kind: Property(Artist)), (kind: Text(" - ")), (kind: Property(Title)), ]), default: (kind: Property(Filename))),
    ],
    header: (
        rows: [
            (
                left: [ (kind: Text("["), style: (fg: "#285981", modifiers: "Bold")), (kind: Property(Status(State)), style: (fg: "#285981", modifiers: "Bold")), (kind: Text("]"), style: (fg: "#285981", modifiers: "Bold")) ],
                center: [ (kind: Property(Song(Artist)), style: (fg: "#367C36", modifiers: "Bold"), default: (kind: Text("Unknown"), style: (fg: "#367C36", modifiers: "Bold"))),
                    (kind: Text(" - ")),
                    (kind: Property(Song(Title)), style: (fg: "#590038", modifiers: "Bold"), default: (kind: Text("No Song"), style: (fg: "#590038", modifiers: "Bold")))
                ],
                right: [ (kind: Text("Vol: "), style: (fg: "#285981", modifiers: "Bold")), (kind: Property(Status(Volume)), style: (fg: "#285981", modifiers: "Bold")), (kind: Text("% "), style: (fg: "#285981", modifiers: "Bold")) ]
            )
        ],
    ),
)
