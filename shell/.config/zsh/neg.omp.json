{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "right",
            "segments": [
                {
                    "foreground": "#899CA1",
                    "leading_diamond": " (",
                    "properties": {
                        "branch_icon": ""
                    },
                    "style": "diamond",
                    "template": "{{ .HEAD }}",
                    "trailing_diamond": ")",
                    "type": "git"
                },
                {
                    "foreground": "#E4F34A",
                    "leading_diamond": " (",
                    "properties": {
                        "fetch_version": false
                    },
                    "style": "diamond",
                    "template": "\ue235{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
                    "trailing_diamond": ")",
                    "type": "python"
                },
                {
                    "foreground": "#7FD5EA",
                    "leading_diamond": " (",
                    "properties": {
                        "fetch_version": false
                    },
                    "style": "diamond",
                    "template": "\ue626{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "trailing_diamond": ")",
                    "type": "go"
                },
                {
                    "foreground": "#42E66C",
                    "leading_diamond": " \ue0b6",
                    "properties": {
                        "fetch_version": false
                    },
                    "style": "diamond",
                    "template": "\ue718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
                    "trailing_diamond": "\ue0b4",
                    "type": "node"
                },
                {
                    "foreground": "#E64747",
                    "leading_diamond": " \ue0b6",
                    "properties": {
                        "fetch_version": false
                    },
                    "style": "diamond",
                    "template": "\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "trailing_diamond": "\ue0b4",
                    "type": "ruby"
                },
                {
                    "foreground": "#E64747",
                    "leading_diamond": " \ue0b6",
                    "properties": {
                        "fetch_version": false
                    },
                    "style": "diamond",
                    "template": "\ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "trailing_diamond": "\ue0b4",
                    "type": "java"
                },
                {
                    "foreground": "#9B6BDF",
                    "leading_diamond": " \ue0b6",
                    "properties": {
                        "fetch_version": false
                    },
                    "style": "diamond",
                    "template": "\ue624{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "trailing_diamond": "\ue0b4",
                    "type": "julia"
                }
            ],
            "type": "rprompt"
        },
        {
            "alignment": "left",
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#005faf",
                    "template": "\ue5ff "
                },
                {
                    "foreground": "red",
                    "powerline_symbol": "\ue0b0",
                    "style": "plain",
                    "template": "\uf0e7 ",
                    "type": "root"
                },
                {
                    "foreground": "#395573",
                    "properties": {
                        "folder_icon": "\ue5ff",
                        "folder_separator_icon": "/",
                        "max_width": 40,
                        "truncation_symbol": "…" ,
                        "cycle": [
                            "#287373",
                            "#899CA1",
                            "#899CA1",
                            "#899CA1",
                            "#899CA1",
                            "#899CA1",
                            "#899CA1",
                            "#899CA1"
                        ]
                    },
                    "template": "{{ .Path }}",
                    "enable_hyperlink": true,
                    "show_git_root": true,
                    "type": "path"
                },
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#005faf",
                    "template": " ❯>"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 3
}
