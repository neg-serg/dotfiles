{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "right",
            "overflow": "hide",
            "segments": [
                {
                    "type": "git",
                    "style": "plain",
                    "foreground": "#899CA1",
                    "properties": {
                        "branch_icon": "",
                        "fetch_status": true,
                        "display_status": true,
                        "display_stash_count": true,
                        "display_upstream_icon": true
                    },
                    "template": "<#005faf>⟮</><#6C7E96>{{ if .Data.HEAD }}<b>{{ .Data.HEAD }}</b>{{ end }}{{ if gt .Data.Ahead 0 }}<#367CB0>←{{ .Data.Ahead }}</>{{ end }}{{ if gt .Data.Behind 0 }}<#367CB0>→{{ .Data.Behind }}</>{{ end }}{{ if and .Data.Working (gt .Data.Working.Changed 0) }} <#914E89>●W{{ .Data.Working.String }}</>{{ end }}{{ if and .Data.Staging (gt .Data.Staging.Changed 0) }} <#5E468C>●S{{ .Data.Staging.String }}</>{{ end }}<#005faf>⟯</>"
                },
                {
                    "type": "executiontime",
                    "style": "powerline",
                    "foreground": "#45bf17",
                    "template": " {{ .FormattedMs }}s <#899CA1>\ue601</>",
                    "properties": {
                        "threshold": 50,
                        "style": "dallas"
                    }
                },
                {
                    "type": "npm",
                    "style": "powerline",
                    "foreground": "#ce162b",
                    "template": "<#899CA1>and</> \ue71e {{ .Full }} "
                },
                {
                    "foreground": "#E4F34A",
                    "properties": {
                        "fetch_version": false
                    },
                    "template": "\ue235{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
                    "type": "python"
                },
                {
                    "foreground": "#7FD5EA",
                    "properties": {
                        "fetch_version": false
                    },
                    "template": "\ue626{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "type": "go"
                },
                {
                    "foreground": "#42E66C",
                    "properties": {
                        "detect_extensions": ["js", "ts"],
                        "fetch_version": true,
                        "cache_duration": "3m"
                    },
                    "template": " \ue718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
                    "type": "node"
                },
                {
                    "foreground": "#48a574",
                    "style": "plain",
                    "template": " <#1e293b>⟮</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#1e293b>⟯</><#899CA1></>",
                    "type": "rust"
                },
                {
                    "foreground": "#316ce4",
                    "style": "plain",
                    "template": "<#1e293b>⟮</>{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}<#1e293b>⟯</><#899CA1></>",
                    "type": "kubectl"
                },
                {
                    "foreground": "#de1f84",
                    "style": "plain",
                    "template": "<#1e293b>⟮</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#1e293b>⟯</><#899CA1></>",
                    "type": "aurelia"
                },
                {
                    "foreground": "#899CA1",
                    "style": "plain",
                    "template": "<#1e293b>⟮</>{{ if .Error }}{{ .Error }}{{ else }}Nx {{ .Full }}{{ end }}<#1e293b>⟯</>",
                    "type": "nx"
                },
                {
                    "foreground": "#ce162b",
                    "properties": {
                        "fetch_version": false
                    },
                    "template": "\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "type": "ruby"
                },
                {
                    "foreground": "#ce162b",
                    "properties": {
                        "fetch_version": false
                    },
                    "template": "\ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "type": "java"
                },
                {
                    "foreground": "#359a25",
                    "properties": {
                        "fetch_version": false
                    },
                    "template": "\ue624{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
                    "type": "julia"
                },
                {
                    "type": "bun",
                    "style": "plain",
                    "foreground": "#007C00",
                    "template": " {{ .Full }}"
                },
                {
                    "type": "deno",
                    "style": "plain",
                    "foreground": "#31658C",
                    "template": "⏣ {{ .Full }}"
                },
                {
                    "type": "zig",
                    "style": "plain",
                    "foreground": "#53A6A6",
                    "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
                },
                {
                    "type": "lua",
                    "style": "plain",
                    "foreground": "#914E89",
                    "template": " {{ .Full }}"
                },
                {
                    "type": "terraform",
                    "style": "plain",
                    "foreground": "#0090FF",
                    "template": " {{ .Full }}"
                },
                {
                    "type": "docker",
                    "style": "plain",
                    "foreground": "#395573",
                    "template": " Docker:{{ .Full }}"
                },
                {
                    "type": "helm",
                    "style": "plain",
                    "foreground": "#005faf",
                    "template": " Helm:{{ .Full }}"
                },
                {
                    "type": "pnpm",
                    "style": "plain",
                    "foreground": "#005fd7",
                    "template": " pnpm:{{ .Full }}"
                },
                {
                    "foreground": "#899CA1",
                    "foreground_templates": ["{{ if gt .Code 0 }}#ce162b{{ end }}"],
                    "properties": {
                        "always_enabled": false
                    },
                    "style": "plain",
                    "template": " ×{{.Code}}",
                    "type": "status"
                }
            ],
            "type": "rprompt"
        },
        {
            "alignment": "left",
            "segments": [
                {
                    "foreground": "#005faf",
                    "properties": {
                        "linux": "<#005faf>\ue27f</>",
                        "macos": "<#005faf>\ue27f</>",
                        "windows": "<#005faf>\ue27f</>"
                    },
                    "style": "plain",
                    "template": "{{ if false }}{{ if .WSL }}WSL at {{ end }} {{.Icon}} {{ end }}",
                    "type": "os"
                },
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#005faf",
                    "template": " "
                },
                {
                    "foreground": "#ce162b",
                    "powerline_symbol": "\ue0b0",
                    "style": "plain",
                    "template": "\uf0e7 ",
                    "type": "root"
                },
                {
                    "foreground": "#395573",
                    "properties": {
                        "folder_separator_icon": "/",
                        "max_width": 40,
                        "home_icon": "<#005faf>~</>",
                        "truncation_symbol": "…" ,
                        "cycle": [
                            "#899CA1",
                            "#899CA1",
                            "#899CA1",
                            "#899CA1",
                            "#899CA1",
                            "#899CA1",
                            "#899CA1"
                        ]
                    },
                    "template": "{{ .Path }}",
                    "enable_hyperlink": true,
                    "show_git_root": true,
                    "type": "path"
                },
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#005faf",
                    "template": " ❯>"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 3
}
